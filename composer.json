{
    "name": "zanphp/zan",
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "http://packagist.qima-inc.com"
        }
    },
    "require": {
        "zan-config/zan": "dev-master",
        "qiniu/php-sdk": "7.0.0",
        "symfony/console": "3.1",
        "elasticsearch/yz-elasticsearch-client": "dev-master",
        "php-lib/nsq-async": "1.2.2",
        "psr/log": "1.0.0",
        "nova-service/generic": "1.0.2"
    },
    "autoload": {
        "psr-4": {
            "Zan\\Framework\\": "src/"
        },
        "classmap": [
        ],
        "files": [
            "src/Foundation/Coroutine/Command/Task.php",
            "src/Utilities/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zan\\Framework\\Test\\": "test/"
        },
        "classmap": [
            "test/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
