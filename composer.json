{
    "name": "zanphp/zan",
    "repositories": [
        {
            "type": "git",
            "url": "http://git.qima-inc.com/php-3rd-lib/qiniu-php-sdk.git"
        },
        {
            "type": "git",
            "url": "http://git.qima-inc.com/php-3rd-lib/symfony-console.git"
        },
        {
            "packagist": false
        }
    ],
    "require": {
        "qiniu/php-sdk": "7.0.0",
        "symfony/console": "3.1"
    },
    "autoload": {
        "psr-4": {
            "Zan\\Framework\\": "src/"
        },
        "classmap": [
        ],
        "files": [
            "src/Foundation/Coroutine/Command/Task.php",
            "src/Utilities/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zan\\Framework\\Test\\": "test/"
        },
        "classmap": [
            "test/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}