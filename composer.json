{
    "name": "zanphp/zan",
    "repositories": [
        {
            "type": "git",
            "url": "git@gitlab.qima-inc.com:php-3rd-lib/qiniu-php-sdk.git"
        },
        {
            "type": "git",
            "url": "git@gitlab.qima-inc.com:php-3rd-lib/symfony-console.git"
        },
        {
            "type": "git",
            "url": "git@gitlab.qima-inc.com:php-3rd-lib/yz-elasticsearch.git"
        },
        {
            "type": "git",
            "url": "git@gitlab.qima-inc.com:php-3rd-lib/psr-log.git"
        },
        {
            "type": "git",
            "url": "git@gitlab.qima-inc.com:php-3rd-lib/monolog-monolog.git"
        },
        {
            "type": "git",
            "url": "git@gitlab.qima-inc.com:php-lib/nsq-async.git"
        },
        {
            "type": "git",
            "url": "git@gitlab.qima-inc.com:php-3rd-lib/pimple-pimple.git"
        },
        {
            "type": "git",
            "url": "git@gitlab.qima-inc.com:php-3rd-lib/guzzle-guzzle.git"
        },
        {
            "type": "git",
            "url": "git@gitlab.qima-inc.com:php-3rd-lib/symfony-event-dispatcher.git"
        },
        {
            "packagist": false
        }
    ],
    "require": {
        "qiniu/php-sdk": "7.0.0",
        "symfony/console": "3.1",
        "elasticsearch/yz-elasticsearch-client": "1.0.0",
        "php-lib/nsq-async": "1.0.2",
        "psr/log": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Zan\\Framework\\": "src/"
        },
        "classmap": [
        ],
        "files": [
            "src/Foundation/Coroutine/Command/Task.php",
            "src/Utilities/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zan\\Framework\\Test\\": "test/"
        },
        "classmap": [
            "test/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
