{
    "name": "zanphp/zan",
    "repositories": [
        {
            "type": "git",
            "url": "http://git.qima-inc.com/php-3rd-lib/qiniu-php-sdk.git"
        },
        {
            "type": "git",
            "url": "http://git.qima-inc.com/php-3rd-lib/league-climate.git"
        },
        {
            "type": "git",
            "url": "http://git.qima-inc.com/php-3rd-lib/seld-cli-prompt.git"
        },
        {
            "type": "git",
            "url": "http://gitlab.qima-inc.com/xiaonuo/yz-elasticsearch.git"
        },
        {
            "packagist": false
        },
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    ],
    "require": {
        "league/climate": "3.2.0",
        "qiniu/php-sdk": "7.0.0",
        "elasticsearch/yz-elasticsearch-client": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "~0.9.2"
    },
    "autoload": {
        "psr-4": {
            "Zan\\Framework\\": "src/"
        },
        "classmap": [
        ],
        "files": [
            "src/Foundation/Coroutine/Command/Task.php",
            "src/Utilities/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zan\\Framework\\Test\\": "test/"
        },
        "classmap": [
            "test/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
